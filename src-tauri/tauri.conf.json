{
  "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/dev/tooling/cli/schema.json",
  "productName": "Mathlib Treemap",
  "version": "0.0.0",
  "identifier": "com.example.mathlib-treemap",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1421/mathlib.html",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Mathlib Treemap",
        "width": 1280,
        "height": 900,
        "url": "mathlib.html"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png",
      "icons/icon-256.png",
      "icons/icon-512.png"
    ]
  }
}
